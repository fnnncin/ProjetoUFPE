<p id="notice"><%= notice %></p>

<h1>Listing Telacads</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telacads.each do |telacad| %>
      <tr>
        <td><%= telacad.nome %></td>
        <td><%= link_to 'Show', telacad %></td>
        <td><%= link_to 'Edit', edit_telacad_path(telacad) %></td>
        <td><%= link_to 'Destroy', telacad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telacad', new_telacad_path %>
